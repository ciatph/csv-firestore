{
  "name": "csv-firestore",
  "version": "1.1.1",
  "description": "Uploads CSV rows as documents to Firestore collections.",
  "main": "index.js",
  "engines": {
    "node": ">=18.20.6",
    "npm": ">=10.8.2"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "node src/index.js",
    "example": "node examples/example.js",
    "example:parser": "node examples/example-parser.js",
    "example:writer": "node examples/example-writer.js",
    "example:firestore": "node examples/example-firestore.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciatph/csv-firestore.git"
  },
